{
  "manifest_version": 2,

  "name": "tabShare",
  "description": "share tabs",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },
  "permissions": [
    "tabs",
    "http://tabshare.tk/*"
  ]
}